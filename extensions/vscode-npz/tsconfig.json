{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "../../",
    "outDir": "out",
    "composite": false,
    "types": [
      "node",
      "vscode"
    ],
    "skipLibCheck": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": [
    "src/**/*.ts",
    "../../src/**/*.ts"
  ]
}
